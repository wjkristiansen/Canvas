{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build Canvas Debug x64",
			"command": "msbuild.exe",
			"args": [
				"Canvas.sln",
				"/p:Configuration=Debug",
				"/p:Platform=x64"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}